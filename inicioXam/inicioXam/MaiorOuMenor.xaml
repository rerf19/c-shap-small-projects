<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="inicioXam.MaiorOuMenor">
    <ContentPage.Content>
        <StackLayout>
            <Entry  x:Name="num1" Keyboard="Numeric" Placeholder="Numero 1"/>
            <Entry  x:Name="num2" Keyboard="Numeric" Placeholder="Numero 2"/>
            <Button x:Name="btnExecutar"  Text="Executar" Clicked="BtnExecutar_Clicked" />
            <Label x:Name="resultado"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>